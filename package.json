  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "autoprefixer": "^10",
    "postcss": "^8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  }
